{
  "name": "peynman/larapress-notifications",
  "description": "Notifications, SMS and E-Mail for Larapress.",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Peyman Abdi",
      "email": "larapress@peynman.ir"
    }
  ],
  "autoload": {
    "psr-4": {
      "Larapress\\Notifications\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Larapress\\Notifications\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Larapress\\Notifications\\Providers\\PackageServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^7.3.0",
    "laravel/framework": "^8.0",
    "nexmo/client": "^2.0",
    "peynman/larapress-crud": "master"
  },
  "require-dev": {
    "facade/ignition": "^2.3.6",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^5.0",
    "orchestra/testbench": "^6.2.0",
    "phpunit/phpunit": "^9.0",
    "phpmd/phpmd" : "@stable",
    "squizlabs/php_codesniffer": "3.*",
    "matthewbdaly/artisan-standalone": "dev-master"
  }
}
